{
  "name": "marktext2web",
  "version": "1.0.0",
  "description": "",
  "author": "625781186 <625781186@qq.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --open",
    "start": "npm run dev",
    "test": "npm run unit",
    "build": "node build/build.js",
    "rebuild": "electron-rebuild -f -o keyboard-layout"
  },
  "build": {
    "productName": "Mark Text",
    "appId": "com.github.marktext.marktext",
    "asar": true,
    "directories": {
      "output": "build"
    },
    "fileAssociations": {
      "ext": [
        "md",
        "markdown",
        "mmd",
        "mdown",
        "mdtxt",
        "mdtext"
      ],
      "role": "Editor"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "icon": "resources/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ],
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "include": "resources/windows/installer.nsh"
    },
    "linux": {
      "category": "Office;TextEditor;Utility",
      "icon": "resources/icons"
    },
    "snap": {
      "confinement": "classic",
      "grade": "stable",
      "plugs": [
        "default",
        "classic-support",
        "wayland"
      ]
    }
  },
  "dependencies": {
    "@hfelix/electron-localshortcut": "^3.1.1",
    "aws-sdk": "^2.466.0",
    "axios": "^0.18.0",
    "babili-webpack-plugin": "^0.1.2",
